node {
	stage 'Checkout'
		checkout scm
	stage 'Build'
		bat 'C:\\Software\\nuget.exe restore Mirabel.MM.Solution.sln'
		bat "\"${tool '.NET Build'}\" Mirabel.MM.Solution.sln /p:VisualStudioVersion=14.0/p:Configuration=Release /p:Platform=\"Any CPU\" /p:ProductVersion=1.0.0.${env.BUILD_NUMBER}"

	stage 'Archive'
		bat 'Echo ToDo archive or working build'
	}